<div class="form-group">
    <label class="form-label" {{#inline}}class="sr-only" {{/inline}}for="{{properties.name}}">{{properties.alias}}</label>
    <select ($change)="onChange(%element.value)" {($value)}="value" id="{{properties.name}}" class="form-select form-control" name="{{name}}">
      {{#each properties.options}}
        <option value="{{value}}">{{label}}</option>
      {{/each}}
    </select>
</div>
